	pages = {174--183},
	file = {Shao 等 - 2017 - High speed rail and urban service industry agglome.pdf:files/2939/Shao 等 - 2017 - High speed rail and urban service industry agglome.pdf:application/pdf;Shao et al_High speed rail and urban service industry agglomeration.pdf:files/2941/Shao et al_High speed rail and urban service industry agglomeration.pdf:application/pdf},
