@article{__2021-8,
	title = {长三角地区城市投资联系水平的时空动态及影响因素},
	volume = {40},
	issn = {1000-0585},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=KPcMkpFoYBDaDCOb-Cx1ZpIgCjJoLHVuFb999BavCadJKvpQLTCEPzIVus5jSs4tv0P4m1RKL5afIw8gvPChaUMMySsCl33-iDDqomxBEoHeAL3EJY1XKtQp8ocEi8Lg&uniplatform=NZKPT&language=gb},
	abstract = {基于2003—2018年企业股权关联投资数据,融合社会网络分析、马尔可夫链分析和空间计量方法,从全球、全国和区域尺度探析长三角地区城市投资联系水平的时空动态特征及影响因素。研究发现:(1)长三角地区各尺度的城际投资规模大幅提升,与关境外地区、关境内地区和长三角区域内的投资联系分别呈现服务业驱动型、制造业驱动型和行业均衡型特征。(2)各尺度城市投资网络呈现区域指向性和多中心演化特征,上海、杭州、南京等在长三角地区逐渐发挥对外辐合全球和全国投资、对长三角地区内部辐射区域的作用。(3)与关境外、关境内和区域内的城市投资联系水平空间分布不均,以沪宁合杭甬发展廊道为支撑的空间结构逐渐凸显,并分别呈现低水平区集中连片分布、改善空间先沿海后内陆扩张、中低水平区广域化扩张的突出特征。(4)城市投资联系水平整体呈现向邻近较高水平方向转移态势,很难实现跨越式转移,存在显著的俱乐部趋同效应和马太效应。随着空间尺度的扩大,向较高水平方向转移的难度随之增大,俱乐部趋同效应和马太效应也随之增强。地理空间格局在各尺度的城市投资联系水平状态转移中发挥着重要作用,发展水平较高和较低的城市对周边城市分别产生正向和负向的空间溢出效应。(5)产业基础、劳动力成本、城市行政级别对各尺度的城市投资联系水平均有显著影响,创新能力对各尺度的城市投资联系水平均未产生显著影响,时间距离对全球和全国尺度的城市投资联系水平影响显著,经济全球化仅对全球尺度的城市投资联系水平产生显著影响。},
	language = {zh},
	number = {10},
	journal = {地理研究},
	author = {高, 鹏 and 何, 丹 and 宁, 越敏 and 韩, 明珑},
	year = {2021},
	note = {26 citations(CNKI)[2024-4-20]},
	keywords = {长三角地区, 影响因素, 城市投资联系水平, 多尺度分析, 企业股权关联投资, 时空动态},
	pages = {2760--2779},
	file = {长三角地区城市投资联系水平的时空动态及影响因素_高鹏.pdf:files/1480/长三角地区城市投资联系水平的时空动态及影响因素_高鹏.pdf:application/pdf},
}

@article{__2014-3,
	title = {中国房价影响因素的区域差异与时序变化研究},
	issn = {2095-5960},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=KPcMkpFoYBAhRkfP58ojtj7Pm-1T0uX4GSm6PM1ExuFQ1546viirODoq1MqNdVM0kIYJ8F4jbaEEKcBkx63FtlvooZdHrNY4CsvgXYbaPOqeb4P3u3PBkw==&uniplatform=NZKPT&language=gb},
	abstract = {采用二阶段局部动态调整模型分析存在市场摩擦和投机时的房价运行特征,进而通过动态面板数据模型和递归分析方法实证分析房价影响因素的区域差异和时序变化。研究结果显示:(1)人口增长并非推动房价上涨的因素;(2)收入因素在东部地区对房价影响不显著;(3)房租对房价仅在西部地区有显著影响;(4)土地成本对房价有影响,在东部影响最强而中部最弱;(5)房价惯性影响在各区域都有显著影响;(6)从时序变化来看,各区域实体经济因素对房价的影响强度都存在下降趋势,而房价惯性的影响却不断上升,反映出投机气氛浓厚使得房价对基本面偏离程度不断提高。因此,我国房地产调控需要建立区域差异化的长效机制。},
	language = {zh},
	number = {1},
	journal = {贵州财经大学学报},
	author = {范, 允奇 and 王, 艺明},
	year = {2014},
	note = {51 citations(CNKI)[2024-4-20]},
	keywords = {影响因素, 区域差异, 房价, 时序变化},
	pages = {62--67},
	file = {中国房价影响因素的区域差异与时序变化研究_范允奇.pdf:files/1548/中国房价影响因素的区域差异与时序变化研究_范允奇.pdf:application/pdf},
}

@phdthesis{__2022-10,
	type = {硕士},
	title = {中国房价影响因素及区域差异研究},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=KPcMkpFoYBAOBIFAZJvf_H0A3gTldPSd5l9lAX-N6CaQeOnjAOvWZ9mmq7htAe3phaY5X3bYWatP3sZCb9LC9ypHMvAiJFwSBm-uHQBzN7e9WN4ZVxZG9w==&uniplatform=NZKPT&language=gb},
	abstract = {住房问题一直是影响中国民众生存品质的重点问题之一,自国家住房保障制度改革以来,中国房价不断攀升。伴随着我国住房市场的迅速发展,虽然宏观调控能够发挥一定的作用,但住房市场发展不平衡不充分的问题也日渐突出。房价在整个社会消费价格中占比较大,一定程度上决定整个市场的消费价格,房价持续发挥调节房地产市场供求结构、控制房地产市场供求总量的重要作用,成为市场经济中重要的调节机制,但各区域对房价调控采取的措施不尽相同,为促进房地产市场健康稳定发展,就需要明确房价的影响因素,以及影响因素的区域差异,为各区域实现房价精准调控提供依据。本研究以2006-2020年全国各省市自治区的房价为出发点,基于资产定价理论和供求决定理论,从经济发展、供给因素和政策因素三个方面进行了房价影响因素的现状分析,通过多元线性回归分析、PVAR模型和脉冲响应分析等方法,探究房价影响因素及区域差异。结果表明:(1)显著影响房价的因素有区域经济、人均可支配收入、房地产开发企业竣工面积和货币供应量,且这些因素对于房价的影响存在区域差异。(2)货币政策对房价影响存在区域差异性。货币供应量对房价产生显著的正向影响,利率则与之相反,两个因素都对西部区域影响最大;住房贷款对东部区域产生显著的负向影响,对西部区域产生显著的正向影响,对中部区域的影响不显著。(3)不同货币政策对东中西区域房价产生的冲击不同。东部区域在利率调控层面反应速度较快,中部、西部区域在货币供应量调控层面反应速度较快。根据研究结果,结合各区域资源与政策现状,提出政府应对不同区域积极实施与其经济相匹配的措施、因地制宜的采取不同政策等建议。},
	language = {zh},
	school = {辽宁大学},
	author = {陈, 茉迪},
	collaborator = {付, 云鹏},
	year = {2022},
	keywords = {区域差异, 房价, 房价影响因素, 脉冲响应, PVAR模型},
	file = {中国房价影响因素及区域差异研究_陈茉迪.pdf:files/1551/中国房价影响因素及区域差异研究_陈茉迪.pdf:application/pdf},
}

@article{__2018-3,
	title = {企业跨区域投资格局及其影响因素——以福建上市企业为例},
	volume = {38},
	issn = {1000-8462},
	doi = {10.15957/j.cnki.jjdl.2018.09.016},
	abstract = {通过Wind金融终端、上市企业年报和国家企业信用信息查询系统收集2010和2015年福建省A股上市企业子公司及对子公司的投资额数据,分析子公司数量和投资额的空间分布及其变化特征;借助地理加权模型探讨福建省上市企业对省外投资的影响因素及其空间差异。研究表明:(1)2010—2015年,福建省上市企业的子公司数量和投资额显著增加,厦门和福州的子公司数量和投资额远高于省内其他城市。(2)福建省内投资网络密度增强,但资金流向不均衡,福建内陆城市对沿海城市的投资额远高于沿海城市对内陆城市的投资额。(3)上市企业在省外的子公司和投资额主要集中在京津冀、长三角和珠三角地区,投资额的地区分布比子公司更加极化。(4)大部分省市的人均GDP、年末常住人口、进出口总额和投资额呈显著正相关,建成区面积和投资额呈现显著负相关,而第三产业比重对投资额的影响较小。整体上,距离福建省越近的地区,解释变量的系数值越稳定,对投资额的解释能力也越强。},
	language = {zh},
	number = {9},
	journal = {经济地理},
	author = {胡国建 and 陈传明 and 陈丽娟 and 王强 and 金星星},
	year = {2018},
	keywords = {空间分布, 福建省, 上市企业, 投资额, 子公司},
	pages = {138--146},
}

