}

@article{tang_understanding_2021,
	title = {Understanding the impact of {High} {Speed} {Railway} on urban innovation performance from the perspective of agglomeration externalities and network externalities},
	volume = {67},
	issn = {0160791X},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0160791X21002359},
	doi = {10.1016/j.techsoc.2021.101760},
	abstract = {Transportation infrastructure and innovation play an important role in promoting regional economic growth. However, previous studies have mainly focused on the economic agglomeration by increased accessibility, but ignored the important on inter-city economic linkages and the network externalities it generated. Based on the externality perspective, this paper uses the propensity score matching model (PSM-DID) and spatial econometric model to analyze the heterogeneous impact of HSR opening on urban innovation performance and compare the differential effects of agglomeration externalities and network externalities on knowledge spillovers. The empirical results suggest the following: (1) The opening of HSR significantly improves urban innovation per­ formance, but there is significant spatial heterogeneity. (2) The opening of HSR not only promotes the agglomeration of production factors to cities, but also strengthens inter-city economic connections, thus improving urban innovation performance through agglomeration externalities and network externalities. How­ ever, compared to agglomeration externalities, network externalities have a superior strength of impact on urban innovation performance. (3) From the dynamic effect perspective, the opening of HSR has a hysteresis effect on urban innovation of 2–3 years, and differences exist between cities.},
	language = {en},
	urldate = {2024-03-18},
	journal = {Technology in Society},
	author = {Tang, Chenghui and Guan, Mingming and Dou, Jianmin},
	month = nov,
	year = {2021},
	pages = {101760},
	file = {Tang 等 - 2021 - Understanding the impact of High Speed Railway on .pdf:files/253/Tang 等 - 2021 - Understanding the impact of High Speed Railway on .pdf:application/pdf},
}

@article{fu_does_2023,
	title = {Does {Urban} {Agglomeration} {Promote} the {Development} of {Cities}? {Evidence} from the {Urban} {Network} {Externalities}},
	volume = {15},
	issn = {2071-1050},
	shorttitle = {Does {Urban} {Agglomeration} {Promote} the {Development} of {Cities}?},
	url = {https://www.mdpi.com/2071-1050/15/12/9850},
	doi = {10.3390/su15129850},
	abstract = {This paper discusses whether urban agglomeration can promote urban growth from the perspective of network externalities. Although agglomeration externalities play an important role in promoting regional development, improved accessibility makes urban network externalities an inﬂuential force in reshaping regional economic growth. This paper identiﬁes the urban network based on the data of train frequency and travel time data among 271 cities in China. Then, the spatial Durbin model is used to investigate the effect of urban agglomeration on economic growth from the perspective of urban network externalities. The results demonstrate that there are signiﬁcant network externalities among cities in China, which play a pivotal role in boosting urban growth. Interactions among cities produce cross-regional spillovers, causing network externalities to no longer be dependent on geographical proximity. This is signiﬁcantly different from agglomeration externalities that are limited to a certain regional scale. As the scope of the urban network expands, network externalities become more pronounced. Different regional characteristics all lead to heterogeneous results of network externalities. The construction of high-speed railways, as well as the well-developed urban network system, strengthen the positive impact of network externalities. Smaller cities beneﬁt more from higher-level urban networks than regional networks.},
	language = {en},
	number = {12},
	urldate = {2024-03-19},
	journal = {Sustainability},
	author = {Fu, Wenfang and Luo, Chuanjian and Yan, Modan},
	month = jun,
	year = {2023},
	pages = {9850},
	file = {Fu 等 - 2023 - Does Urban Agglomeration Promote the Development o.pdf:files/264/Fu 等 - 2023 - Does Urban Agglomeration Promote the Development o.pdf:application/pdf},
}

@article{ning_urban_2016,
	title = {Urban innovation, regional externalities of foreign direct investment and industrial agglomeration: {Evidence} from {Chinese} cities},
	volume = {45},
	issn = {00487333},
	shorttitle = {Urban innovation, regional externalities of foreign direct investment and industrial agglomeration},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0048733316300075},
	doi = {10.1016/j.respol.2016.01.014},
	abstract = {This paper examines how industrial agglomeration interacts with the intra- and inter-regional externalities resulting from foreign direct investment (FDI) in urban innovation in an emerging economy. It adds to the existing literature by highlighting the importance of considering both spatial proximity and urban industrial structures in understanding FDI knowledge spillovers in urban areas. Using a unique and manually collected city-level dataset for the period from 2005 to 2011 in China, our empirical results confirm the role of FDI as an important external knowledge source in the context of a developing country. The spatial externalities of FDI, however, are rather limited to the city of investment. We further show that FDI spatial spillovers are contingent upon the intensity of industrial agglomeration within and across cities. Specialized cities absorb FDI knowledge spillovers within the regions and also facilitate their dissemination to nearby regions, while diversified cities provide a vibrant environment for local innovation. Our empirical evidence has important implications for both inconclusive theoretical debates and policy making.},
	language = {en},
	number = {4},
	urldate = {2024-03-19},
	journal = {Research Policy},
	author = {Ning, Lutao and Wang, Fan and Li, Jian},
	month = may,
	year = {2016},
	pages = {830--843},
	file = {Urban innovation, regional externalities of foreign direct investment and industrial agglomeration\: Evidence from Chinese cities:files/309/ning2016.pdf.pdf:application/pdf},
}

@article{lu_influence_2022,
	title = {The influence of high-speed rails on urban innovation and the underlying mechanism},
	volume = {17},
	issn = {1932-6203},
	url = {https://dx.plos.org/10.1371/journal.pone.0264779},
	doi = {10.1371/journal.pone.0264779},
	abstract = {Innovation is intrinsically dependent on the construction of local infrastructure. Using panel data on 285 cities in China, we empirically examined the impact of high-speed rails on urban innovation and the mechanism underlying this effect. We found that high-speed rails significantly increase urban innovation. In our analysis, high-speed rails were found to increase the agglomeration of innovation factors, including population and investment, which in turn increase urban technological innovation. The agglomeration of investment factors brought about by high-speed rails is the main source of the improvement in urban innovation.},
	language = {en},
	number = {3},
	urldate = {2024-03-19},
	journal = {PLOS ONE},
	author = {Lu, Yue and Yang, Siying and Li, Jian},
	editor = {Yang, Jun},
	month = mar,
	year = {2022},
	pages = {e0264779},
}

@article{fan_does_2023,
	title = {Does high-speed railway promote urban innovation? {Evidence} from {China}},
	volume = {86},
	issn = {00380121},
	shorttitle = {Does high-speed railway promote urban innovation?},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0038012122002658},
	doi = {10.1016/j.seps.2022.101464},
	abstract = {Empirical discussion in the existing literature on the relationship between transportation infrastructure and innovation remains limited. As one of the most important transport infrastructures, China’s high-speed railway (HSR) has greatly compressed the space-time distance and strengthened the linkages between cities, which may contribute to innovation activities. Using the panel data of 285 Chinese prefecture-level and above cities and employing the difference-in-differences (DID) model, this paper examines the impact of HSR opening and HSR service intensity on the urban innovation. Propensity score matching (PSM) together with DID (PSM-DID) method is utilized to address the potential estimation biases. The empirical results demonstrate that HSR has significantly improved the level of urban innovation. Heterogeneity analysis finds that the promotion effects of HSR on innovation are more remarkable in peripheral and small cities. Moreover, the effects of HSR on cities far away from the central and large cities are with higher significance and greater magnitude than cities close to central and large cities. For cities near central and large cities, the service intensity of HSR is more likely to improve their innovation level. In addition, we further find that service industries and private enterprises benefit more from the effect of HSR in promoting innovation. This study can provide robust evidence for the effect of HSR on promoting urban innovation, as well as policy enlightenment for innovation growth and sustainable economic development.},
	language = {en},
	urldate = {2024-03-19},
	journal = {Socio-Economic Planning Sciences},
	author = {Fan, Xiaomin and Xu, Yingzhi},
	month = apr,
	year = {2023},
	pages = {101464},
	file = {Fan 和 Xu - 2023 - Does high-speed railway promote urban innovation .pdf:files/312/Fan 和 Xu - 2023 - Does high-speed railway promote urban innovation .pdf:application/pdf},
}

@phdthesis{__2021-1,
	type = {硕士},
	title = {高铁开通对技术创新外溢的影响机制与实证研究},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=DFdco8SIy0IcVuvLqPb_fdUdTzOzxw3TWce65HZXGR5cEpqnO0QEUgd2STC3yIwwMz0lb-pvMc8wXunIPm07-YKAEzmkywoXeQZylVCSchkjsVvqPb0J-A==&uniplatform=NZKPT&language=gb},
	abstract = {高铁被誉为中国“新四大发明”之一,高铁提高了区域可达性,扩大了区域经济活动的空间范围。自中国进入“高铁网络”建设时代,高铁在区域经济发展中发挥着越来越大的作用。研究表明交通基础设施具有正外部性,高铁相比公路和铁路而言,对区域经济发展的具有更大的空间外溢效应。我国经济在高速发展过程中伴随着区域经济发展不均衡等问题,技术创新外溢在缓解我国区域经济发展不均、促进我国经济增长等方面具有重要的作用。欠发达地区可以通过发达地区技术创新外溢搭上技术进步的“便车”,缩短与发达地区的差距。而技术创新外溢与人力资本、资金等要素的流动息息相关,人力资本的流动伴随着知识尤其是隐性知识的传播和扩散,资本的流动促进了创新要素的聚集和扩散。高铁的开通打破了区域间的空间壁垒,扩大了城市的经济活动半径,增强了要素的流动性。本文重点关注了高铁开通是否加速了技术创新外溢这一主题。在研究视角方面,以往针对科技创新问题的科学研究,多数是对科技创新进行探讨,对由于科技创新外部性产生的科技创新外溢的研究较少。同样,现有研究高铁经济的文献,对于全要素增长率、区域经济一体化以及要素流动等方面的研究已相对全面。但对于高铁网络建设与创新驱动经济发展相结合的研究相对较少。因此,本文从高铁开通的视角对区域技术创新外溢展开研究,丰富了交通基础设施建设的相关研究。此外,在研究的内容方面,目前对高铁经济展开的研究大多是从省级层面或针对某条高铁线展开的,研究方法大都采用面板或空间面板模型。样本量较少且应用双重差分的研究少。本文创新性的选取专利相关数据对技术创新外溢进行了测度,应用得分倾向匹配和双重差分的计量方法进行研究,使研究更加稳健、可信。基于此,本文采用2008-2013年230个地级市的面板数据分析了高铁开通对区域技术创新外溢的影响。研究发现:(1)高铁开通提升了专利引用率,同时加速了专利的折旧速度,即高铁开通促进了区域技术创新外溢。(2)高铁通过促进集聚经济、面对面交流、人才和资本流动以及区域合作贸易等方式增强了技术创新的外溢效应,且高铁开通对技术创新外溢的影响具有时滞性,时滞约为2-3年;(3)技术创新外溢具有空间异质性。要素流动是异质性的主要原因,据此本文对政府和企业分别提出了相关建议。},
	language = {zh},
	school = {南京财经大学},
	author = {庄, 海涛},
	collaborator = {余, 泳泽},
	year = {2021},
	keywords = {创新外溢, 高铁开通, 失效专利, 专利引用},
	file = {高铁开通对技术创新外溢的影响机制与实证研究_庄海涛.pdf:files/337/高铁开通对技术创新外溢的影响机制与实证研究_庄海涛.pdf:application/pdf},
}

@article{__2022-4,
	title = {集聚外部性、网络外部性与城市创新发展},
	volume = {41},
	issn = {1000-0585},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=DFdco8SIy0Ie6mgREH8VtxgkFAdKKv2XSZvQsvEq34Pb9wnQKyOGBYEhtegJETs44O5WkJe-9DCvMEzdJvC9fsAzJ5aSPwQedO7i-3aGen7Uv65yCWj0JNkD63XruEFa&uniplatform=NZKPT&language=gb},
	abstract = {随着高速交通技术和移动基础设施的迅速发展，城市创新活动已经不再局限于特定的地理空间，在网络空间中彼此远离但有着较强经济活动关联的城市之间是否也可以实现协同创新，目前仍缺乏经验证据。基于2001—2016年中国289个地级市宏观面板数据和微观层面上市公司数据，采用社会网络分析法、工具变量法以及空间杜宾模型等方法，定量研究了集聚外部性与网络外部性对于城市创新发展的影响。结果显示：创新要素依旧存在着向头部企业和中心城市集中的趋势，且正是这种集中所带来的集聚外部性促进了城市的创新发展；城市可通过融入经济关联网络，借助于网络外部性来提高自身创新水平；就集聚外部性与网络外部性促进城市创新发展的传导机制来看，虽然政府科教支出水平、城市基础设施状况以及高铁网络的建设是集聚外部性与网络外部性发挥的重要条件，但经济活动密度的提升所带来的集聚外部性还可以强化网络外部性所带来的创新效应，而外商直接投资活动所分享的先进技术和管理经验则帮助网络外部性突破了地理空间的限制。因此，政府在探索城市创新发展路径的过程中，就无需囿于创新要素的空间集聚这一条路径，积极融入城市群内的国际科技创新中心建设，凭借网络外部性所分享的新知识与新技术，同样也能实现创新发展。},
	language = {zh},
	number = {9},
	journal = {地理研究},
	author = {姚, 常成 and 吴, 康},
	year = {2022},
	note = {42 citations(CNKI)[2024-3-19]},
	keywords = {创新发展, 集聚外部性, 企业网络, 投资网络, 网络外部性},
	pages = {2330--2349},
	file = {集聚外部性、网络外部性与城市创新发展_姚常成.pdf:files/402/集聚外部性、网络外部性与城市创新发展_姚常成.pdf:application/pdf},
}

@article{__2023-2,
	title = {集聚外部性、网络外部性与城市经济发展},
	volume = {37},
	issn = {1003-7578},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=DFdco8SIy0KbaLzYgxhTm98V-YcIFaXx0BkwZmWjn_mGUQFAz_tOo2RXifXHbI4CA-0tGxyPBZqIUW1EPLUmLlI5e272kTT6hUiG_bg6MJD5wZvLUw3vyMPe0lUviKq7&uniplatform=NZKPT&language=gb},
	doi = {10.13448/j.cnki.jalre.2023.135},
	abstract = {基于外部性视角，探究集聚外部性、网络外部性对城市经济发展的影响，对于推动区域高质、可持续发展具有重要意义。利用2020年百度指数、人口迁徙、列车通行、经济联系等数据构建城市综合网络，采用空间计量模型分析集聚外部性、网络外部性对城市经济发展的影响机理。结果表明：1)中国城市网络联系呈明显的“小世界”特征，主要联系分布在“胡焕庸线”东侧；城市经济发展具有明显的空间正相关特征，自东向西呈“热点-过渡-冷点”阶梯状结构。2)城市经济发展具有明显的正向空间溢出效应，集聚外部性和网络外部性均能对城市经济发展产生显著影响，其中集聚外部性(专业化)具有正向影响效应，网络外部性存在“集聚阴影”效应。3)集聚外部性、网络外部性对城市经济发展的影响存在区域差异，集聚外部性对东北地区经济发展的影响最为明显，网络外部性对东部地区经济发展产生显著影响。},
	language = {zh},
	number = {6},
	journal = {干旱区资源与环境},
	author = {刘, 贺 and 李, 雪铭 and 田, 深圳 and 李, 松波 and 孙, 赫},
	year = {2023},
	note = {1 citations(CNKI)[2024-3-19]},
	keywords = {集聚外部性, 网络外部性, 经济发展, 空间计量模型, 中国},
	pages = {38--45},
	file = {集聚外部性、网络外部性与城市经济发展_刘贺.pdf:files/407/集聚外部性、网络外部性与城市经济发展_刘贺.pdf:application/pdf},
}

@phdthesis{__2023-3,
	type = {硕士},
	title = {高铁发展水平对城市创新能力的影响},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=DFdco8SIy0IdLHF3-4pC97TG7iFDFhyEGGhlSeOQfOr7Fg63MpmMpi15RUjW_wHIvw0neK9QqpufL4-sZJINW8qxEERMrHVWkHpKtHU8DsqnI4SmQLoDRA==&uniplatform=NZKPT&language=gb},
	abstract = {党的十九大报告指出,创新是引领发展的第一动力,是建设现代化经济体系的战略支撑,到2035年,我国经济实力、科技实力将大幅跃升,跻身创新型国家前列。然而,当前我国不同地区间的创新水平存在巨大差异,创新活动集中于大城市。随着人才、技术、资本等各类生产要素不断地加速向城市大规模集聚,城市已然成为了企业创新活动的重要载体,如何积极发挥城市在创新中的引领作用也逐渐成为创新驱动经济发展方式转型、建设创新型国家的重要环节。而高铁作为一种高效能的交通工具,极大程度的缩短了城市间的时空距离,降低了运输和商务交流的时间成本,减少了城市之间知识流动的阻碍,推进了要素聚集及城市的融合,从而提升高铁沿线中小城市的创新水平。研究高铁发展水平对城市创新能力的影响有助于对我国大规模高铁建设进行全面评价、有助于优化创新发展战略的实施举措。本文以截至2016年已实现高铁通车的191个地级市为研究对象,基于2009-2016年191个地级市的面板数据,采用空间计量的研究方法,就高铁发展水平对城市创新能力的影响进行了分析。从高铁联系广度和高铁联系强度两方面来反应高铁发展水平,利用复旦大学产业发展研究中心发布的《中国城市和产业创新力报告2017》中城市创新水平的数据来刻画城市创新能力,构建空间杜宾模型,研究了高铁发展水平的提升能够加强人才交流、提升人力资本水平,进而增强城市整体的创新氛围,使得城市创新能力实现突破化的进展,从而为创新举措的完善带来新的思路。研究发现,高铁发展水平的提高能够显著的促进城市创新能力的提升,且具备显著的空间溢出效应。交通水平的改善能直接的凸显人才流动及知识的溢出效应,营造了城市创新活动的良好氛围,带来更密集的创新活动,加速技术的进步,从而带动了城市创新能力的提升。同时从高铁联系广度和高铁联系强度上看,高铁联系广度和高铁联系强度都对城市创新能力的有着显著正向的促进作用,且高铁联系广度的提升比高铁联系强度的提升对城市创新能力提升的效果更为明显。},
	language = {zh},
	school = {山东大学},
	author = {谢, 宛汀},
	collaborator = {张, 进峰},
	year = {2023},
	keywords = {城市创新能力, 高铁发展水平, 高铁联系广度, 高铁联系强度, 空间杜宾模型},
	file = {高铁发展水平对城市创新能力的影响_谢宛汀.pdf:files/412/高铁发展水平对城市创新能力的影响_谢宛汀.pdf:application/pdf},
}

@article{__2018,
	title = {从集聚外部性走向跨越地理边界的网络外部性——集聚经济理论的回顾与展望},
	volume = {25},
	issn = {1006-3862},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=DFdco8SIy0KRJqVIS5ujUvtiVsW48oKB_SO96aEN3gpJt5Sup1OERogPBevsfB4w-ieIAZChbskIz5q-Ck0T7KroZvZEjSa-MqZv-Ivp2jRNJEPDEwK-3ZfYVQmHmxS6&uniplatform=NZKPT&language=gb},
	abstract = {集聚经济理论是近百年来区域科学领域最热点的理论之一。在回顾集聚外部性相关研究的基础上,综述网络外部性的早期探索及近期研究进展。在集聚外部性视角下,新古典区位理论、城市经济学、新经济地理学、企业异质性理论和演化经济地理学等领域相继对集聚经济理论作出理论贡献。但是,集聚外部性的研究局限于特定的地域范围内,在关注的地理尺度和测度方法等方面存在不足。近年来,面向跨越地理边界的集聚经济研究迅速发展。借用规模、集聚外部性域、集聚阴影、城市网络外部性等网络外部性视角下的理论相继提出。当前,前沿研究集中在网络外部性能否替代集聚外部性、网络外部性的测度方法和网络外部性的异质性等方面。最后,从网络外部性理论的融合与扩展、分析方法及政策启示等方面提出研究展望。},
	language = {zh},
	number = {12},
	journal = {城市发展研究},
	author = {林, 柄全 and 谷, 人旭 and 王, 俊松 and 毕, 学成},
	year = {2018},
	note = {61 citations(CNKI)[2024-3-19]},
	keywords = {集聚外部性, 网络外部性, 集聚经济, 跨越地理边界},
	pages = {82--89},
	file = {从集聚外部性走向跨越地理边...—集聚经济理论的回顾与展望_林柄全.pdf:files/442/从集聚外部性走向跨越地理边...—集聚经济理论的回顾与展望_林柄全.pdf:application/pdf},
}

@article{_230_2019,
	title = {高铁开通是否加速了技术创新外溢？—来自中国230个地级市的证据},
	volume = {45},
	issn = {1001-9952},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=DFdco8SIy0J-7tgB2kUrg1h8rj8mD2EWiV--jhO1tzEbVHoX_PM36vOAWE5c8H-FstQ3raAelABZbuY1sFF4R9F-UPEDnroOn0w4aHgcMGO2d6sASDIEgGKMu90s2isD&uniplatform=NZKPT&language=gb},
	doi = {10.16538/j.cnki.jfe.2019.11.002},
	abstract = {高铁的出现压缩了区域之间的"时空"距离,极大地促进了区域间要素的流动。文章重点关注了高铁开通是否加速了技术创新的外溢这一主题,并采用2008-2013年中国230个地级市的面板数据,通过双重差分法(DID)及倾向得分匹配-倍差法(PSM-DID)分析了高铁开通对区域技术创新外溢的影响。研究发现:(1)高铁开通提升了人均专利引用量,同时加速了专利的折旧速度。高铁通过促进经济集聚、区域间合作与贸易以及企业间学习等方式增强了技术创新的外溢效应,且高铁开通对技术创新外溢的影响时滞大约为2-3年。(2)高铁开通对技术创新外溢的影响具有一定的区域异质性。由于东部地区经济发展程度较高,加之高铁网络下的资源聚集作用,因此高铁开通对技术创新外溢的影响在东部地区更加显著。因此对于政府来说,各城市需要继续完善关键性的基础设施,为高铁建设提供各项技术与物质的支撑;对于企业来说,企业应充分利用高铁开通所带来的"时空压缩"效应,加大与跨城企业的合作与贸易,提高自身的创新水平并积极学习其他企业的技术。},
	language = {zh},
	number = {11},
	journal = {财经研究},
	author = {余, 泳泽 and 庄, 海涛 and 刘, 大勇 and 伏, 雨},
	year = {2019},
	note = {112 citations(CNKI)[2024-3-19]},
	keywords = {创新外溢, 高铁开通, 失效专利, 专利引用},
	pages = {20--31+111},
	file = {余泳泽 等 - 2019 - 高铁开通是否加速了技术创新外溢？—来自中国230个地级市的证据.pdf:files/446/余泳泽 等 - 2019 - 高铁开通是否加速了技术创新外溢？—来自中国230个地级市的证据.pdf:application/pdf},
}

@article{komikado_high-speed_2021-1,
	title = {High-speed rail, inter-regional accessibility, and regional innovation: {Evidence} from {Japan}},
	volume = {167},
	issn = {00401625},
	shorttitle = {High-speed rail, inter-regional accessibility, and regional innovation},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0040162521001293},
	doi = {10.1016/j.techfore.2021.120697},
	abstract = {This study investigates the association between accessibility and regional innovation in Japan through an empirical analysis that uses 322 prefecture-level seven-year panel data drawn from 46 prefectures covering 1981 to 2014. It assumes that regional innovation is represented by the annual number of patent applications in a region, while accessibility is represented by the effective density (ED) and existence of high-speed rail (HSR) stations in the region. Regional knowledge productivity functions are estimated by incorporating the existence of HSR stations and ED as explanatory variables to examine the associations between accessibility and regional innovation. The results show that the existence of HSR stations has a positive and statistically significant asso­ ciation with knowledge productivity. We also observe a significant interaction effect of these two variables, implying that the association of HSR stations with regional knowledge productivity is positive only in lower ED cases. Furthermore, our results reveal that the agglomeration of the finance industry has a positive association with regional innovation. Policy implications from the findings are discussed, such as the indirect effect of transportation infrastructure development on regional innovation through accessibility and the importance of implementing transportation development strategies jointly with regional industrial development.},
	language = {en},
	urldate = {2024-03-19},
	journal = {Technological Forecasting and Social Change},
	author = {Komikado, Hiroshi and Morikawa, So and Bhatt, Ayushman and Kato, Hironori},
	month = jun,
	year = {2021},
	pages = {120697},
	file = {Komikado 等 - 2021 - High-speed rail, inter-regional accessibility, and.pdf:files/454/Komikado 等 - 2021 - High-speed rail, inter-regional accessibility, and.pdf:application/pdf},
}

@article{__2019,
	title = {高铁开通是否促进了区域创新?},
	issn = {1002-7246},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=DFdco8SIy0Ig-EJ0SqKBAfTSBJnuttdT9vwPOfOXp-sFoRoBqTfKbvyTnLSl6sdkHCPtTaZhlCiIT25RFeRYvsAlKY4v_anv5bwDVQnvHCXNniApRUPklK6DrSjD6PqX&uniplatform=NZKPT&language=gb},
	abstract = {文章通过理论分析,探讨了高铁开通影响区域创新活动以及区域创新差距的内在机制,进而以是否开通高铁为"准自然实验",采用2004年-2015年中国287个地级市的数据,实证分析了高铁开通对区域创新及其差距的影响效应。研究发现,考察期内,高铁开通显著提升了区域创新水平,其机制主要在于高铁开通后所引发的创新要素流动效应;高铁开通能够对区域创新差距产生重要影响:即相对于未开通高铁的城市来说,开通高铁城市的创新能力和创新速度持续提升,这能够进一步拉大区域之间的创新差距;且高铁开通在促进区域创新过程中也存在着显著的时间动态效应。分地区估计结果显示,高铁开通能够促进东部地区城市创新活动的开展,也拉大了其内在的创新差距;而由于高铁开通所引发的人才流失,高铁开通对中部和西部地区创新活动及其差距的影响效应不显著。文章研究结论可能为优化中国区域创新格局、促进创新型国家建设提供启示。},
	language = {zh},
	number = {6},
	journal = {金融研究},
	author = {卞, 元超 and 吴, 利华 and 白, 俊红},
	year = {2019},
	note = {376 citations(CNKI)[2024-3-20]},
	keywords = {区域创新, 创新差距, 高铁, 双重差分模型},
	pages = {132--149},
	file = {高铁开通是否促进了区域创新__卞元超.pdf:files/484/高铁开通是否促进了区域创新__卞元超.pdf:application/pdf},
}

@phdthesis{__2020-2,
	type = {硕士},
	title = {高铁网络、外部性和中国城市经济增长},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=DFdco8SIy0LqB_q71YWCs7wM4UIfl83nLnOth4U2geMBSyykY4KOFcRJlruSgr_IqCTcyWXgZQEeO5IYkrN56FQA72O9ciqUQ5-hpX4VPhP0KYFuLBD6ug==&uniplatform=NZKPT&language=gb},
	abstract = {城市群的协调发展不仅取决于资源的空间配置,还取决于城市之间的相互联系和作用,即城市之间的外部性。基于空间维度的外部性可以分为集聚外部性和网络外部性,本文在高铁网络快速发展背景下研究高铁外部性对我国城市经济的影响。高铁外部性的特殊性在于其集聚外部性是俱乐部性质的,其网络外部性对距离又是敏感的。因此本文首先论述高铁的集聚外部性和网络外部性之间的相互联系;其次,通过构建T小时俱乐部市场潜力指数来衡量高铁对城市经济影响的外部性;再次,运用面板数据固定效应法对中国高铁网络中高铁经济圈的有效作用范围进行实证研究;最后,根据高铁对短期人员流动(客流量)和较长期的人口流动(就业量)的影响大小来探讨高铁通行经济圈发挥有效范围背后的原因。研究发现从整体上看,城市可以发挥外部性(集聚和网络)的有效经济规模主要为3-5小时的中短距离范围;高铁的开通运营一方面对总客运量和铁路客运量有明显的正向影响,而对民用飞机客运量的有显著的负向影响,说明高铁在促进人口短期流动上发挥了重要作用;另一方面,也通过双重差分法发现,高铁的开通运营对就业也有一定的促进作用但并不明显,且存在滞后效应。因此,本文发现高铁外部性作用于城市经济主要通过促进短期客运流动,而不是长期就业迁移来发挥作用,高铁外部性的最强距离范围是四小时高铁运行时间。},
	language = {zh},
	school = {厦门大学},
	author = {左, 佳},
	collaborator = {李, 静},
	year = {2020},
	keywords = {高铁网络, 市场潜力指数, 外部性},
	file = {高铁网络、外部性和中国城市经济增长_左佳.pdf:files/496/高铁网络、外部性和中国城市经济增长_左佳.pdf:application/pdf},
}

@article{__2020-3,
	title = {高铁开通对区域创新格局的影响及其作用机制},
	issn = {1000-6249},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=Vof-4b7nxdDP6WvZP6QueWIogx9AnCrs07FYSctyFS3Lbjg3PGRzi8duwHcmIaWtSsYc8fmjgUZ4A09WWfApyWbRL5xpnG9pXQQF-rYDdHZB37Gij8XDOYeJphDGzDqt&uniplatform=NZKPT&language=gb},
	doi = {10.19592/j.cnki.scje.370950},
	abstract = {交通基础设施是区域创新体系的重要组成部分,创新活动对交通基础设施具有内在依赖性。从理论上讲,作为联接不同城市的重要交通方式,高铁开通为城市创新活动提供了诸多便利条件,会促进高铁城市创新网络的形成,加快知识传播与溢出,提升城市创新水平,并缩小高铁城市之间创新差距。文章基于我国2003年至2016年285个城市面板数据,运用双重差分模型和空间面板计量模型,实证分析了高铁开通对区域创新空间格局的影响及其作用机制。研究发现,高铁开通提高了城市通达性,强化了高铁城市内部知识与技术溢出,带动了城市创新要素集聚,并对区域创新空间格局产生深刻影响。一方面,高铁开通对城市创新的影响具有"俱乐部效应",促进了高铁城市内部科技创新的均衡化发展,缩小高铁城市创新差距;另一方面,高铁开通也会强化高铁城市与非高铁城市科技创新的二元化发展趋势,拉大两者创新水平差距。},
	language = {zh},
	number = {5},
	journal = {南方经济},
	author = {杨, 思莹 and 李, 政},
	year = {2020},
	note = {67 citations(CNKI)[2024-3-20]},
	keywords = {高铁开通, 城市创新, 创新网络, 通达性, 要素集聚},
	pages = {49--64},
	file = {高铁开通对区域创新格局的影响及其作用机制_杨思莹.pdf:files/504/高铁开通对区域创新格局的影响及其作用机制_杨思莹.pdf:application/pdf},
}

@article{__2022-5,
	title = {中国城市群城市经济增长的网络外部性及其作用机制},
	volume = {43},
	issn = {1000-7636},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=Vof-4b7nxdCiV9rATslteCydG3OEQphIRvcYLkKwQnVRuwVEIr8NC1iSW6svTc-ya1o2CXUP1gtwb0xByolR9pD0DuqgT8m5OCAoWXoQteS2f1jkSdRdEDYMx1PlsXnW&uniplatform=NZKPT&language=gb},
	doi = {10.13502/j.cnki.issn1000-7636.2022.02.004},
	abstract = {中国城市群将成为未来中国城镇化的主要空间载体，也是未来中国经济增长的重要引擎，而城市网络外部性是挖掘城市群经济增长潜力的关键途径。本文基于城市网络外部性的理论构建城市群城市经济增长的模型，运用空间计量的实证方法，利用2003—2019年中国12个城市群178个地级及以上城市平衡面板数据，实证检验中国城市群城市经济增长的网络外部性及其作用机制。实证结果表明，相比城市群外的城市网络外部性，城市群内的劳动力和技术产生更强的正向城市网络外部性，且城市群内部的高铁可达性可以强化劳动力的正向城市网络外部性，城市群内部的产业关联度可以强化技术的正向城市网络外部性。根据本文结论，加强城市群内城市间联系，完善城市群内交通基础设施，促进城市群内城市间产业关联，是中国城市群未来实现城市经济协同增长的主要途径。},
	language = {zh},
	number = {2},
	journal = {经济与管理研究},
	author = {张, 洪鸣 and 孙, 铁山},
	year = {2022},
	note = {13 citations(CNKI)[2024-3-20]},
	keywords = {城市网络, 城市经济增长, 城市网络外部性, 城市群, 高铁可达性},
	pages = {48--64},
	file = {中国城市群城市经济增长的网络外部性及其作用机制_张洪鸣.pdf:files/527/中国城市群城市经济增长的网络外部性及其作用机制_张洪鸣.pdf:application/pdf},
}

@article{__2021-2,
	title = {城市网络外部性研究述评},
	volume = {40},
	issn = {1007-6301},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=Vof-4b7nxdDVlAnZ5z80UYv2Gzc7eL9Z6inO88_vqVHjXh6Xl9I7LiOJdqq1ya7ERV-47pyceBxh5Y-3xdfA_DYN1a_GOU9IYLus9iWJ1PieOruKQwRTqS6Ghyk58xut&uniplatform=NZKPT&language=gb},
	abstract = {城市网络研究已成为国际上城市研究的前沿学术领域并渐成热点,外部性作为城市网络的本质属性,对城市网络的演化及城市与区域的发展均具有重要意义。论文从集聚外部性与网络外部性比较的视角切入,重点对城市网络外部性形成机理、效用和测度方法等方面的研究进展进行梳理和评述。其中,协同效应、整合效应和规模借用被认为是城市网络外部性形成的重要原因。而城市网络外部性的效用研究较多集中在2个方面:一是关注要素流动对知识扩散和创新的促进作用,二是关注城市网络对竞争力提升和经济增长的影响。从已有文献来看,城市网络外部性测度研究主要涉及识别和估算2种类型,包括3种常用方法:关联分析、回归分析和空间计量分析。未来需要进一步关注的问题包括城市网络的外部性理论认识、外部性量度方法以及经验研究问题。},
	language = {zh},
	number = {4},
	journal = {地理科学进展},
	author = {程, 玉鸿 and 苏, 小敏},
	year = {2021},
	note = {48 citations(CNKI)[2024-3-21]},
	keywords = {城市网络, 外部性, 区域发展, 述评},
	pages = {713--720},
	file = {程 和 苏 - 2021 - 城市网络外部性研究述评.pdf:files/541/程 和 苏 - 2021 - 城市网络外部性研究述评.pdf:application/pdf},
}

