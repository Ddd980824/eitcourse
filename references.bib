@article{__2022-9,
	title = {我国房地产市场分化与区域经济增长},
	volume = {18},
	issn = {1672-996X},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=KPcMkpFoYBBAVlzX9BJJdKFgeZ_F9j6RqWaEV2zaWe9KNf-jp0EJBMOc7i_y8vclhhpW5cY1cakWq1YiymxPKNyCxDhYRIQgdMw2EN-GgpsOwTKw9CkCwSA0xhOx4z8S&uniplatform=NZKPT&language=gb},
	abstract = {2003年以来,我国房地产市场快速发展,我国房地产市场也呈现出明显的分化。本研究运用基尼系数和泰尔指数,对主要城市群间的房地产市场发展差异水平进行了测度,并分析了区域间人均土地出让金、住房价格、人均房地产投资额的分化程度。结合经验事实对比分析,进一步探析房地产市场区域分化对区域经济发展的影响。研究发现,不同区域内部的人均土地出让金及住房价格存在分化特征,这与区域经济的发展分化有一定的相关性,适度的房地产市场分化对经济增长会产生一定的推动作用。京津冀城市群房价水平的不平等程度与其他两个城市群没有显著差异,但内部经济发展差异最大。以京津冀城市群为例,房价分化程度如果与经济发展差异水平不匹配,则会影响人口流动,对区域经济发展产生阻碍作用。因此,应关注房地产市场的供求平衡,优化房地产供给,避免过高的房价导致劳动力流失而损害经济的长期健康发展。},
	language = {zh},
	number = {3},
	journal = {科技促进发展},
	author = {陈, 然 and 高, 歌 and 骆, 娜 and 李, 秀婷 and 高, 鹏},
	year = {2022},
	keywords = {房地产市场, 基尼系数, 区域差异, 区域经济, 泰尔指数},
	pages = {302--312},
	file = {陈 et al_2022_我国房地产市场分化与区域经济增长.pdf:files/1457/陈 et al_2022_我国房地产市场分化与区域经济增长.pdf:application/pdf},
}

@article{__2014-2,
	title = {中国房价影响因素的区域差异与时序变化研究},
	issn = {2095-5960},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=KPcMkpFoYBDFoWgfZh0HzxCCginoQVPGDszRI3-rrQU0zpOj6AT4vkMrnrJaYSR3HpKdlLy_PhphdwTBMmQzZC0pezm0BvxvMIHnA5H0d1gXWeQNONTBfA==&uniplatform=NZKPT&language=gb},
	abstract = {采用二阶段局部动态调整模型分析存在市场摩擦和投机时的房价运行特征,进而通过动态面板数据模型和递归分析方法实证分析房价影响因素的区域差异和时序变化。研究结果显示:(1)人口增长并非推动房价上涨的因素;(2)收入因素在东部地区对房价影响不显著;(3)房租对房价仅在西部地区有显著影响;(4)土地成本对房价有影响,在东部影响最强而中部最弱;(5)房价惯性影响在各区域都有显著影响;(6)从时序变化来看,各区域实体经济因素对房价的影响强度都存在下降趋势,而房价惯性的影响却不断上升,反映出投机气氛浓厚使得房价对基本面偏离程度不断提高。因此,我国房地产调控需要建立区域差异化的长效机制。},
	language = {zh},
	number = {1},
	journal = {贵州财经大学学报},
	author = {范, 允奇 and 王, 艺明},
	year = {2014},
	note = {51 citations(CNKI)[2024-4-20]},
	keywords = {影响因素, 区域差异, 房价, 时序变化},
	pages = {62--67},
	file = {中国房价影响因素的区域差异与时序变化研究_范允奇.pdf:files/1461/中国房价影响因素的区域差异与时序变化研究_范允奇.pdf:application/pdf},
}

@phdthesis{__2017-1,
	type = {硕士},
	title = {我国房地产市场价格的区域差异及其影响因素研究},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=KPcMkpFoYBBt1NxuiXvnT3dbrgJ4otPkdUe7KaY9hOAMUccBsYI3Pcss-QWAlTlZmx_lFJmtOkzVPRUpQjBArmhTh5X0PfXiSBVpKMAZ6gUbNwTRJ-lwlw==&uniplatform=NZKPT&language=gb},
	abstract = {1998年,我国取消住房实物分配制度,自此开始,房地产市场化开始在我国真正实施,进入21世纪之后,我国房地产市场日趋繁荣,房地产市场为全国经济发展注入了强力针,并逐渐成为全国经济的支柱产业之一。然而,在我国房地产市场繁荣的景象之下,房价的过快增长产生了一些经济问题和社会问题,为了控制房价的过快增长,政府接连出台了多项房地产调控政策,然而宏观调控的效果并不如预期,并且不同地区的调控也产生了不同的效果。深究其中的原因,“一刀切”的房地产宏观调控政策可能是主要原因之一。因此,本文主要研究我国房地产市场价格区域差异的原因,从房地产市场供求角度选取影响因素建立指标体系,希望找到不同地区房地产市场价格区域差异的原因,为政府因地制宜的进行房地产宏观调控提供政策建议,也对房地产企业有效的制定投资组合、分散非系统风险有一定的指导意义。本文对我国房地产市场价格区域差异的原因进行研究,同时采用了定性分析法和定量分析法,首先阐述房地产市场相关理论和国内外研究现状,再用定性分析的方法理论描述我国房地产市场区域差异的现状,对我国房地产市场不同区域的总体特征、空间特征和速度差异进行测度,接着在我国房地产市场区域差异现状的基础上从需求和供给的角度选取影响房地产市场价格区域差异的主要因素,并对这些因素对房地产价格的影响路径做理论上的分析,最后建立数据面板模型,选取我国35个大中城市的房价和影响因素的数据作为样本,并将其分为东中西三个地区分别进行研究。通过分析发现我国房地产市场的确存在较大差异,东中西部三个地区房价的主要影响因素不同,东部地区房价主要受供给因素影响,而收入因素对其房价的影响不如其他地区,中部地区房价同时受到供求两方面因素的影响,包括收入因素和建设成本因素,而西部地区主要受需求因素的影响,尤其是收入因素的影响程度最大。各地区影响因素的不同也要求当地政府采取相应的房地产调控政策,在本文的最后也针对东中西三个地区提供了不同的房地产发展政策。},
	language = {zh},
	school = {重庆大学},
	author = {潘, 子康},
	collaborator = {梁, 平},
	year = {2017},
	note = {4 citations(CNKI)[2024-4-20]},
	keywords = {影响因素, 房地产区域差异, 面板数据模型},
	file = {我国房地产市场价格的区域差异及其影响因素研究_潘子康.pdf:files/1464/我国房地产市场价格的区域差异及其影响因素研究_潘子康.pdf:application/pdf},
}

@phdthesis{__2019-5,
	type = {硕士},
	title = {中国房地产市场区域差异研究},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=KPcMkpFoYBDuBDlh5Ibj-GbHhcqLaa0-G7PWd1jROCYoByL9dlMQ2qHLdCEeL2hU5N22zdiK7biZ_834QFO19rDpevApMDImM_8d19ognuM93AdMFzWjqA==&uniplatform=NZKPT&language=gb},
	abstract = {1998年,住房分配货币化和住房供应商品化的体制改革在中国拉开大幕,从此房地产市场进入发展快速阶段。房地产逐步发展成为支柱产业并推动着我国国民经济的发展,一方面,房地产以其强大的产业带动性促进了一系列相关产业的发展,促进了国民经济的高速发展,另一方面,一旦房地产市场出现问题,其产业带动性会反过来拖累其他行业的发展从而影响国民经济。房地产天然是个区域性的市场,促进房地产市场的发展不能采取一刀切的形式,而应该分析不同区域房地产市场的特征,采取差异化的发展策略。本文先从理论角度对房地产市场发展差异的原因进行推测研究,然后通过聚类分析将285个城市分成了 5类区域进行研究,首先初步分析得出经济因素、金融因素、基础设施因素和政策因素是引起全国285个城市之间房地产市场差异的主要原因,再将5类区域房地产市场主要影响因素进行进一步空间计量分析并发现,随着经济发展,房地产发展自身的空间溢出效应逐渐减弱,城市的国民生产总值(GDP)对周边城市的房地产发展的负作用逐渐增强,但是城市的金融状况对其周边城市的房地产发展有着促进作用,最后根据不同影响因素对于房地产市场发展作用的大小,相应地提出了不同的发展建议。本文的主要贡献在于,在因变量的选取上,不同于大多数文献仅将房地产价格作为因变量进行分析,本文将从房价、销售面积、就业、投资额四个角度出发构建了一个综合指标,以衡量区域的房地产市场发展状况。在样本范围上,本文将针对中国285个地级市进行分析,研究范围更为全面,得出的结论不仅适用于全国,也适用各省,更具有普遍意义。在计量模型上,考虑了不同城市之间的地理相关性,采用空间计量经济学的方法进行研究,将相邻地区的影响考虑在内,更符合现实的情况。},
	language = {zh},
	school = {厦门大学},
	author = {胡, 梦飞},
	collaborator = {蒙, 莉娜},
	year = {2019},
	note = {2 citations(CNKI)[2024-4-20]},
	keywords = {区域差异, 房地产, 实证研究},
	file = {中国房地产市场区域差异研究_胡梦飞.pdf:files/1467/中国房地产市场区域差异研究_胡梦飞.pdf:application/pdf},
}

@phdthesis{__2011-5,
	type = {硕士},
	title = {我国房地产市场的区域差异及影响因素研究},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=KPcMkpFoYBCrJnGQdOAmn8YJyvm49hSULSfGcXJEyMiZ6-0QffNAiMQwjg5gUj8GEkUkG5wDW_5LghdNGNWSu3dVtEdl7dCrTQR8KTy0EchNCJyPQ_ttEg==&uniplatform=NZKPT&language=gb},
	abstract = {目前,我国房地产市场存在着较为严重的区域不平衡。主要表现在某些地区,如北京、上海、深圳等一线城市房地产市场投资过热,房价上涨过快,投资规模过大；而其他地区房地产市场发展程度较低。同时,东部区域内部之间房地产市场也存在不同的差异,中西部内部也有着不同的差异。本文首先采用聚类分析方法,制定了房地产市场综合指标体系,采用2006-2008年的数据,将我国35个大中城市分为六大区域。分析不同区域的房地产市场的市场规模、市场结构、供求关系、市场发展阶段和资金来源的差异。研究发现我国35大中城市房地产市场的主要区域差异在于房价和房地产市场发展阶段。然后分析形成宏观经济因素对我国房地产价格影响的区域差异。采用面板数据模型,将聚类分析结果的六大区域调整为了三大区域,采用2002-2008年35个大中城市作为样本。结论发现房地产国内贷款对三个区域房价的影响都很显著,而且影响较大。而人均可支配收入和居民人均储蓄存款对三个区域的影响也是显著的,但影响程度次于房地产贷款。本文的研究意义在于,首先政府可以根据我国房地产市场的区域差异,因地制宜的制定房地产市场宏观调控政策,并且通过调节房地产市场的影响因素来调控当地房地产市场。其次,对于房地产企业来讲,通过研究和区分我国35个大中城市的区域差异,可以帮助制定未来的市场投资规划,有效的组合不同区域的房地产投资和开发策略,有效的分散非系统风险,而且更加深刻认识宏观经济因素对当地房地产市场的影响。},
	language = {zh},
	school = {华东师范大学},
	author = {李, 巧波},
	collaborator = {崔, 裴},
	year = {2011},
	note = {24 citations(CNKI)[2024-4-20]},
	keywords = {房地产市场, 影响因素, 区域差异},
}

