@phdthesis{__2022-7,
	type = {硕士},
	title = {投资视角下中国城市网络的空间演化特征及其机制研究},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=b4E8SuETvlLagKKBC9qM8ayjaQL7zhaJ-1H6KtTsQ7_WYu3g6djFSG8ZiqMbojiNt3QANDUOhPlmlpwiulNNBZ6_qgWqqtj4g42O2-iFrIkAuiOnjba9lg==&uniplatform=NZKPT&language=gb},
	abstract = {我国在计划经济时代,城市横向之间的联系被人为阻断;90年代我国的社会主义市场经济体制开始改革;进入21世纪,我国的社会主义市场化经济改革完成,开始进入产业振兴时期,企业之间的跨区域投资行为逐渐涌现,流空间逐渐形成,城市网络逐渐发育。在此背景下,本文希望在投资视角下对进入21世纪的中国城市网络的演化特征及其机制进行研究。通过理论和文献的综述,可以发现城市网络研究目前呈现两级化的特征,一方面,我国的城市网络研究进入白热化阶段,是城市研究和地理科学研究的一个重要热点。另一方面,我国的城市网络研究也遇到一些研究瓶颈和挑战。为此,本文以2019年《财富》中国500强企业为研究对象,通过Python爬取这些企业截至2019年的跨区域投资数据,汇总成“城市对”之间的投资数据并基于该数据构建有向加权的城市网络。在此基础上,以1999年、2009年和2019年三个时间点,分三次产业对形成的城市网络从“点-线-面”三个维度研究归纳城市网络的空间演变特征并进行比较分析。之后,通过参考人口迁移中间障碍模型构建城市网络空间演化的解释机制模型,先从动因演化进行分析,再分别解释进入21世纪之后三次产业形成的城市网络的空间演变的内在逻辑和机制。主要进行以下分析:在城市网络演化特征方面。点维度方面主要从出度、入度、中介中心性和Page Rank四个方面对三次产业进行分析,线维度方面主要通过网络密度、链接强度、平均聚集系数和链接对称性四个方面进行分析,面维度方面主要通过社团划分和首位社团的组织模式两个方面进行分析,最终发现三次产业的空间演化特征较为明显且差异较大。在城市网络演化机制方面。首先参考人口迁移中间障碍模型构建了动因-结果解释机制模型。之后先分析动因演化,即产业特性演化是城市网络演化呈现各种特征的底层逻辑,城市-资金诉求演化是导致城市网络演化呈现各种特征的直接原因,本质上是产业特性演化的外在空间呈现。在对动因分析的基础上进一步对三次产业的演化机制分别进行解释。最后提出相应的政策建议。主要得到以下结论:城市中心性具有等级性、可变性和复杂性,其演化受城市综合实力和个性特征综合影响,其中综合实力强的城市中心性普遍较强,而个性特征也会随着时间和产业类型的变化进而影响城市中心性变化。因此,本文建议一方面要洞察产业发展的逻辑,前瞻性布局产业发展需要的新的基础配套,从而突出城市个性,抢占发展先机;另一方面,要动态关注城市在网络中的中心性,研究城市在网络中承担的功能、角色和诉求,从而不断调整城市的发展目标和定位。城市间链接的演化主要受产业发展阶段和城市-资金诉求的综合影响,其中产业在产生、成长、进化等发展阶段会导致该产业的资金链接在量和方向上改变,而城市与资金的双向诉求互相作用也会影响资金流的量和方向;因此,本文认为城市积极洞察产业发展趋势,前瞻性地布局产业发展的新配套有利于迎合城市或资金的青睐而产生链接,从而加强与其他城市的联系;此外,提高城市基础设施的通达性,尤其是网络设施建设和虚拟通道的通达性,有利于助力资金流动,疏通链接。社团的演化是产业特性和城市间联系的综合影响,所谓产业特性是指不同类型产业在产业链特征、劳动地域分工等方面的特殊性会影响社团的划分和演化,城市间的联系强度和方向也会影响社团的组织结构和特征。因此,本文建议探索区域多元化的治理新模式,例如通过成立跨区域协调部门、探索跨区域合作机制、搭建跨区域沟通平台等方式来有效促进城市间的跨区域协作。本文正文共约50000字,图表51幅。},
	language = {zh},
	school = {南京大学},
	author = {闫, 文奇},
	collaborator = {葛, 幼松},
	year = {2022},
	keywords = {城市网络, 空间演化, 机制, 投资视角},
	file = {投资视角下中国城市网络的空间演化特征及其机制研究_闫文奇.pdf:files/999/投资视角下中国城市网络的空间演化特征及其机制研究_闫文奇.pdf:application/pdf},
}

@article{__2021-6,
	title = {长三角赛博空间信息流动的方向性与层级性},
	volume = {30},
	issn = {1004-8227},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=b4E8SuETvlKdHH6IG1yOcTd-VwmrtPs9E7V6IuvaQA8FSrKiZW8uf7WdBcykiz1ocQzCTG8IHRPWJT2ZQyupcLCCqD1LBAoWRjZ_IQxTn-uBTLprg2pmZlBovFLf567Z&uniplatform=NZKPT&language=gb},
	abstract = {作为实体空间的补充,赛博空间的研究为解读区域空间组织方式提供新的视角。基于百度指数信息流数据,通过分析城市出入度、城际出入流和节点区域的特征,揭示了2018年长三角赛博空间信息流动的方向性与层级性。结果显示:(1)从出入度来看,上海、杭州、苏州、南京兼具信息资源的辐射能力和集聚能力;合肥在长三角赛博空间的主导性较强但信息虹吸能力弱;出入度均低的城市主要位于安徽省内。(2)从出入流来看,杭州的强信息辐射效应主要在省内;南京的信息辐射集中在沪宁杭沿线;苏州的辐射效应集中在沿海城市;上海对江浙城市有较强的信息辐射;而安徽城市则通过南京融入长三角信息流,整体参与度较低。(3)从节点区域来看,长三角赛博空间形成了以上海为绝对中心、南京为次级中心、合肥为三级中心的跨区域等级性节点区域和以杭州为中心、宁波为次级中心的浙江本地节点区域。在此基础上,研究进一步分析其影响因素并提出相应的政策建议。},
	language = {zh},
	number = {9},
	journal = {长江流域资源与环境},
	author = {戴, 靓 and 纪, 宇凡 and 邵, 蕊 and 蔡, 翡翡 and 何, 秀婷},
	year = {2021},
	note = {4 citations(CNKI)[2024-3-27]},
	keywords = {长三角, 百度指数, 点出入度, 节点区域, 赛博空间, 信息流动},
	pages = {2070--2078},
	file = {戴 et al_长三角赛博空间信息流动的方向性与层级性_2021.pdf:files/1002/戴 et al_长三角赛博空间信息流动的方向性与层级性_2021.pdf:application/pdf},
}

