@article{capello_city_2000,
	title = {The {City} {Network} {Paradigm}: {Measuring} {Urban} {Network} {Externalities}},
	volume = {37},
	copyright = {https://journals.sagepub.com/page/policies/text-and-data-mining-license},
	issn = {0042-0980, 1360-063X},
	shorttitle = {The {City} {Network} {Paradigm}},
	url = {https://journals.sagepub.com/doi/10.1080/713707232},
	doi = {10.1080/713707232},
	abstract = {In recent years, network behaviour has been analysed extensively as the emerging model for economic growth. By network behaviour, a metaphor for co-operative behaviour among individuals, corporate or territorial partners is intended. This is increasingly becoming the reference paradigm in an era of continuing innovation and fast technological change, in the presence of ‘market failure’ where dynamic and innovative behaviours are concerned and of the high costs of a growth strategy based solely on internal know-how. The theory of the city network paradigm claims that, through participation in the network, cities exploit scale economies in complementary relationships and synergies in co-operative activities. In this sense, network advantage is a real club good, achieved only by those economic actors who are partners in the economic and spatial network, and is distributed among partners despite the private marginal costs each partner bears to participate in the network. In this sense, the private marginal costs of network participation differ from private marginal bene ts, and network advantages turn out to be network externalities. The aim of the present paper is to measure the impacts that city network behaviour has on city performance—i.e. to provide a quantitative measurement of network externalities stemming from network behaviour in territorial systems.},
	language = {en},
	number = {11},
	urldate = {2025-02-19},
	journal = {Urban Studies},
	author = {Capello, Roberta},
	month = oct,
	year = {2000},
	pages = {1925--1945},
	file = {Capello - 2000 - The City Network Paradigm Measuring Urban Network.pdf:files/2923/Capello - 2000 - The City Network Paradigm Measuring Urban Network.pdf:application/pdf;Capello_The City Network Paradigm.pdf:files/2925/Capello_The City Network Paradigm.pdf:application/pdf},
}

@article{derudder_conceptual_2006,
	title = {On {Conceptual} {Confusion} in {Empirical} {Analyses} of a {Transnational} {Urban} {Network}},
	volume = {43},
	copyright = {https://journals.sagepub.com/page/policies/text-and-data-mining-license},
	issn = {0042-0980, 1360-063X},
	url = {https://journals.sagepub.com/doi/10.1080/00420980600897842},
	doi = {10.1080/00420980600897842},
	abstract = {A number of researchers have recently tried to map the contours of a transnational urban network. Although these empirical studies have great merits in making a closer connection to theoretical ideas on a genuine urban network, they sometimes fail to recognise that the whole idea of cities as ‘nodes’ in a transnational urban network is a heuristic at best. To assess this underdeveloped analytical connection, a taxonomy of dominant empirical and theoretical approaches is constructed. Contrasting both taxonomies reveals a mismatch between theory and measurement. This mismatch is addressed in more detail through four separate but entwined examples of conceptual conﬂation: a lack of rigour in the employed terminologies; inappropriate discussions of results against the background of other concepts; the inadequate delineation of the urban area; and, the limited analytical value of infrastructure-based analyses.},
	language = {en},
	number = {11},
	urldate = {2025-02-19},
	journal = {Urban Studies},
	author = {Derudder, Ben},
	month = oct,
	year = {2006},
	pages = {2027--2046},
	file = {Derudder - 2006 - On Conceptual Confusion in Empirical Analyses of a.pdf:files/2926/Derudder - 2006 - On Conceptual Confusion in Empirical Analyses of a.pdf:application/pdf;On Conceptual Confusion in Empirical Analyses of a Transnational Urban Network:files/2928/derudder2006.pdf.pdf:application/pdf},
}

