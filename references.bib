	number = {5},
	journal = {科研管理},
	author = {党, 兴华 and 张, 晨},
	year = {2022},
	note = {6 citations(CNKI)[2024-7-4]},
	keywords = {风险投资, 邻近性, 声誉, 网络嵌入性, 网络演化, STERGM},
	pages = {182--190},
